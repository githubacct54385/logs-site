<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
  <div class="jumbotron">
    <h1>Logs</h1>
<table class="table">
    <thead>
      <tr>
        <th>Time</th>
        <th>Message</th>
      </tr>
    </thead>
    <tbody>
      <% logs.forEach(function(log) { %>
      <tr>
              <td><%= log.createdAt %></td>
              <td><%= log.message %></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  </div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>
